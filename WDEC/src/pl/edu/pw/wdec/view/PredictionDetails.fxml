        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="30.0" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="30.0" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="399.0" minHeight="10.0" prefHeight="399.0" valignment="TOP" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="priceField" GridPane.columnIndex="1" />
            <TextField fx:id="qualityField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Cena" />
            <Label text="Jakość" GridPane.rowIndex="1" />
            <Button fx:id="computeButton" mnemonicParsing="false" onAction="#handleComputeButtonAction" text="Policz" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <GridPane.margin>
